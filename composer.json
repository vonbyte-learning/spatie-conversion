{
    "name": "vonbyte/spatie-conversion",
    "description": "Perform conversions",
    "keywords": [
        "vonbyte",
        "spatie-conversion"
    ],
    "homepage": "https://github.com/vonbyte/spatie-conversion",
    "license": "MIT",
    "authors": [
        {
            "name": "Tanja von Byte",
            "email": "info@vonbyte.ch",
            "homepage": "https://vonbyte.ch",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpunit/phpunit": "^9.0",
        "vimeo/psalm": "^3.11"
    },
    "autoload": {
        "psr-4": {
            "VonByte\\UnitConversions\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VonByte\\UnitConversions\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
